<mvc:View
    controllerName="zqm.controller.Main"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:sfb="sap.ui.comp.smartfilterbar"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:table="sap.ui.table"
    xmlns:l="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:html="http://www.w3.org/1999/xhtml"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <sfb:SmartFilterBar
            id="smartFilterBar"
            entitySet="ZC_QueryVendor"
            basicSearchFieldName="Lifnr"
            persistencyKey="UniqueAndStablePersistencyKey"
        >
            <sfb:controlConfiguration>
                <sfb:ControlConfiguration
                    id="_IDGenControlConfiguration1"
                    key="Lifnr"
                    label="Fournisseur"
                    visibleInAdvancedArea="true"
                    filterType="single"
                />
                <sfb:ControlConfiguration
                    id="_IDGenControlConfiguration2"
                    key="class"
                    label="Classe"
                    visibleInAdvancedArea="true"
                    filterType="single"
                />
            </sfb:controlConfiguration>
        </sfb:SmartFilterBar>
 <html:a id="hiddenLink"></html:a> 
        <table:Table
            id="table"
            rows="{/}"
            selectionMode="MultiToggle"
            paste="onPaste"
            ariaLabelledBy="title"
        >
            <table:extension>
                <OverflowToolbar
                    id="_IDGenOverflowToolbar1"
                    style="Clear"
                >
                    <Title
                        id="title"
                        text="Fournisseurs"
                    />
                    <ToolbarSpacer id="_IDGenToolbarSpacer1"/>
                    <Button id="_IDGenButton2"
							icon="sap-icon://refresh"
							tooltip="Effacer tous les filtres"
							press="onClearAllFilters"/>
					<Button id="_IDGenButton1" type="Accept" press="onExtractData" icon="sap-icon://excel-attachment"/>
                </OverflowToolbar>
            </table:extension>
            <table:columns>
                <!-- Name1 -->
                <table:Column
                    id="columnName1"
                    width="11rem"
                    sortProperty="Name1" filterProperty="Name1"
                >
                    <Label
                        id="labelName1"
                        text="{i18n>Name1}"
                    />
                    <table:template>
                        <Text
                            id="textName1"
                            text="{Name1}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- Lifnr -->
                <table:Column
                    id="columnLifnr"
                    width="11rem"
                    sortProperty="Lifnr" filterProperty="Lifnr"
                >
                    <Label
                        id="labelLifnr"
                        text="{i18n>Lifnr}"
                    />
                    <table:template>
                        <Text
                            id="textLifnr"
                            text="{Lifnr}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- CATEGORIE_FOURNISSEUR -->
                <table:Column
                    id="columnCategorieFournisseur"
                    width="11rem"
                    sortProperty="CategorieFournisseur" filterProperty="CategorieFournisseur"
                >
                    <Label
                        id="labelCategorieFournisseur"
                        text="{i18n>CATEGORIE_FOURNISSEUR}"
                    />
                    <table:template>
                        <Text
                            id="textCategorieFournisseur"
                            text="{CategorieFournisseur}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- QUALIFICATION -->
                <table:Column
                    id="columnQualification"
                    width="11rem"
                    sortProperty="Qualification" filterProperty="Qualification"
                >
                    <Label
                        id="labelQualification"
                        text="{i18n>QUALIFICATION}"
                    />
                    <table:template>
                        <Text
                            id="textQualification"
                            text="{Qualification}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- ENTENTE_QUALITE -->
                <table:Column
                    id="columnEntenteQualite"
                    width="11rem"
                    sortProperty="EntenteQualite" filterProperty="EntenteQualite"
                >
                    <Label
                        id="labelEntenteQualite"
                        text="{i18n>ENTENTE_QUALITE}"
                    />
                    <table:template>
                        <Text
                            id="textEntenteQualite"
                            text="{EntenteQualite}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- CATEGORIE_FOURNISSEUR old -->
                <table:Column
                    id="columnCategorieFournisseurOld"
                    width="11rem"
                    sortProperty="CategorieFournisseurOld" filterProperty="CategorieFournisseurOld"
                >
                    <Label
                        id="labelCategorieFournisseurold"
                        text="{i18n>CATEGORIE_FOURNISSEUR_OLD}"
                    />
                    <table:template>
                        <Text
                            id="textCategorieFournisseurold"
                            text="{CategorieFournisseurOld}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- QUALIFICATION old -->
                <table:Column
                    id="columnQualificationold"
                    width="11rem"
                    sortProperty="QualificationOld" filterProperty="QualificationOld"
                >
                    <Label
                        id="labelQualificationold"
                        text="{i18n>QUALIFICATION_OLD}"
                    />
                    <table:template>
                        <Text
                            id="textQualificationOld"
                            text="{QualificationOld}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- ENTENTE_QUALITE old -->
                <table:Column
                    id="columnEntenteQualiteOld"
                    width="11rem"
                    sortProperty="EntenteQualiteOld" filterProperty="EntenteQualiteOld"
                >
                    <Label
                        id="labelEntenteQualiteOld"
                        text="{i18n>ENTENTE_QUALITE_OLD}"
                    />
                    <table:template>
                        <Text
                            id="textEntenteQualiteOld"
                            text="{EntenteQualiteOld}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- Ernam -->
                <table:Column
                    id="columnErnam"
                    width="11rem"
                    sortProperty="Ernam" filterProperty="Ernam"
                >
                    <Label
                        id="labelErnam"
                        text="{i18n>Ernam}"
                    />
                    <table:template>
                        <Text
                            id="textErnam"
                            text="{Ernam}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- Erdat -->
                <table:Column
                    id="columnErdat"
                    width="11rem"
                    sortProperty="Erdat" filterProperty="Erdat"
                >
                    <Label
                        id="labelErdat"
                        text="{i18n>Erdat}"
                    />
                    <table:template>
                        <Text
                            id="textErdat"
                            text="{path: 'Erdat', formatter: '.formatDate'}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- Username -->
                <table:Column
                    id="columnUsername"
                    width="11rem"
                    sortProperty="Ernam" filterProperty="Ernam"
                >
                    <Label
                        id="labelUsername"
                        text="{i18n>Username}"
                    />
                    <table:template>
                        <Text
                            id="textUsername"
                            text="{Username}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>

                <!-- Udate -->
                <table:Column
                    id="columnUdate"
                    width="11rem"
                    sortProperty="Udate" filterProperty="Udate"
                >
                    <Label
                        id="labelUdate"
                        text="{i18n>Udate}"
                    />
                    <table:template>
                        <Text
                            id="textUdate"
                            text="{path: 'Udate', formatter: '.formatDate'}"
                            wrapping="false"
                        />
                    </table:template>
                </table:Column>
                

            </table:columns>
        </table:Table>

        <BusyDialog id="BusyDialog" />
    </Page>
</mvc:View>
